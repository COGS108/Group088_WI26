    "input_path = 'data/00-raw/steamspy.json'\n",
    "output_dir = 'data/02-processed'\n",
    "os.makedirs(output_dir, exist_ok=True)\n",
    "output_path = os.path.join(output_dir, 'steamspy_clean.csv')\n",
    "\n",
    "with open(input_path, 'r', encoding='utf-8') as f:\n",
    "    data = json.load(f)\n",
    "\n",
    "df = pd.DataFrame.from_dict(data, orient='index')\n",
    "df.reset_index(drop=True, inplace=True)\n",
    "print('Raw shape:', df.shape)\n",
    "df.head()\n",
    "\n",
    "columns_needed = [\n",
    "    'appid',\n",
    "    'name',\n",
    "    'developer',\n",
    "    'publisher',\n",
    "    'initialprice',\n",
    "    'ccu',\n",
    "    'positive',\n",
    "    'negative',\n",
    "    'average_2weeks'\n",
    "]\n",
    "df = df[columns_needed]\n",
    "print('Columns after tidying:')\n",
    "df.head()\n",
    "\n",
    "numeric_cols = [\n",
    "    'initialprice',\n",
    "    'ccu',\n",
    "    'positive',\n",
    "    'negative',\n",
    "    'average_2weeks'\n",
    "]\n",
    "for col in numeric_cols:\n",
    "    df[col] = pd.to_numeric(df[col], errors='coerce')\n",
    "\n",
    "df['initialprice'] = df['initialprice'] / 100\n",
    "df.head()\n",
    "\n",
    "missing_summary = df.isnull().sum()\n",
    "print('Missing data per column:\\n', missing_summary)\n",
    "missing_percent = df.isnull().mean() * 100\n",
    "print('\\nPercentage missing per column:\\n', missing_percent)\n",
    "df.describe()\n",
    "\n",
    "outlier_threshold = df['ccu'].quantile(0.99)\n",
    "outliers = df[df['ccu'] > outlier_threshold]\n",
    "print('Top 1% CCU outliers:\\n', outliers[['name', 'ccu']])\n",
    "\n",
    "df['total_reviews'] = df['positive'] + df['negative']\n",
    "df['estimated_revenue'] = df['total_reviews'] * df['initialprice']\n",
    "df['positive_review_percentage'] = df['positive'] / df['total_reviews']\n",
    "df['positive_review_percentage'] = df['positive_review_percentage'].replace([np.inf, -np.inf], np.nan)\n",
    "df.rename(columns={'average_2weeks': 'avg_playtime_2weeks'}, inplace=True)\n",
    "df.head()\n",
    "\n",
    "df = df.dropna(subset=['initialprice', 'ccu', 'total_reviews'])\n",
    "print('Cleaned dataset shape:', df.shape)\n",
    "\n",
    "df.to_csv(output_path, index=False)\n",
    "print('Final cleaned dataset saved to:', output_path)\n"
 ]
},
